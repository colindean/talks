    "og-image": "marp codemash2020.md -o dist/og-image.jpg",
    "start": "marp -ps ."
  },
  "devDependencies": {
    "@marp-team/marp-cli": "^2.2.2",
    "cpx": "^1.5.0",
    "rimraf": "^3.0.0"
  }
}
